@model IEnumerable<NutriHub.Models.IngestaDiaria>
@{
    ViewBag.Title = "Ingesta diaria";
    Layout = "~/Views/Shared/_LayoutPa.cshtml";
}

<h2>Ingesta diaria del paciente</h2>

<table class="table">
    <tr>
        <th>
            VET
        </th>
        <th>
            Porcentaje
        </th>
        <th>
            Nombre
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.vet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.porcentaje)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nutriente1.nombre)
            </td>
        </tr>
    }

</table>